
<div class="container-fluid form-horizontal">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-title">
    <h4>Reporting - Create Form</h4>
   </div>
            <div class="card-body">
    <form action="/reporting/create" method="post">
    
    
    
    
    
    
    <div class="form-group  row">
        <div class="col-md-10">
            <label for="publicationDate" class = "col-sm-2 col-form-label">Publication Date</label>
            <div class="col-sm-10">
            <input type="date" id="publicationDate" name="publicationDate" class="form-control">
            </div>
        </div>
    </div>
    
    
    
    
    <div class="form-group  row">
        <div class="col-md-10">
            <label for="content" class = "col-sm-2 col-form-label">Content</label>
            <div class="col-sm-10">
            <input type="text" id="content" name="content" class="form-control">
            </div>
        </div>
    </div>
    
    
    
    
    <div class="form-group  row">
        <div class="col-md-10">
            <label for="type" class = "col-sm-2 col-form-label">Type</label>
            <div class="col-sm-10">
            <input type="text" id="type" name="type" class="form-control">
            </div>
        </div>
    </div>
    
    
    
    
    <div class="form-group  row">
        <div class="col-md-10">
            <label for="results" class = "col-sm-2 col-form-label">Results</label>
            <div class="col-sm-10">
            <input type="number" id="results" name="results" class="form-control">
            </div>
        </div>
    </div>
    
    
    
    
    <div class="form-group  row">
        <div class="col-md-10">
            <label for="views" class = "col-sm-2 col-form-label">Views</label>
            <div class="col-sm-10">
            <input type="integer" id="views" name="views" class="form-control">
            </div>
        </div>
    </div>
    
    
    
    
    <div class="form-group  row">
        <div class="col-md-10">
            <label for="idCard" class = "col-sm-2 col-form-label">Id Card</label>
            <div class="col-sm-10">
            <input type="integer" id="idCard" name="idCard" class="form-control">
            </div>
        </div>
    </div>
    
    
<div class="form-group row form-action">
    <div class="col-sm-10">
        <button type="submit" class="btn btn-primary btn-sm">Create</button>
    </div>
</div>
    </form>
 </div>
        </div>
    </div>
</div>

